                <th width="150">Amt</th>
                <th width="150">Paid By</th>
              </tr>
            </thead>
            <tbody>
              <% @current_expenses.each do |exp| %>
                <tr>
                  <td><%= exp.type %></td>
                  <td><%= exp.amount %></td>
                  <td><%= exp.get_display_name %></td>
                <% end %>
                </tr>
            </tbody>
          </table>
