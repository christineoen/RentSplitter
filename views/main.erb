              </tr>
            </thead>
            <tbody>
              <% @domicile_users.each do |user| %>
              <tr>
                <td><%= user.display_name %></td>
                <td><%= user.rent %></td>
                <td><%= @reallocated_domicile[user.user_id] %></td>
                <td><%= user.rent.to_i + @reallocated_domicile[user.user_id].to_i %></td>
                <% end %>
              </tr>
            </tbody>
          </table>
        </div>  
